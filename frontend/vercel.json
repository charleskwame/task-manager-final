{
    "version": 2,
    "builds": [
        {
            "src": "frontend/**/*.js", // Or your specific entry file
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist" // Your Vite build output directory
            }
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}